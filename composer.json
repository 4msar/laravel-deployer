{
    "name": "4msar/laravel-deployer",
    "description": "Zero-downtime deployment tool for Laravel applications from GitHub releases",
    "keywords": [
        "laravel",
        "deployer",
        "deployment",
        "github",
        "releases",
        "zero-downtime",
        "cli"
    ],
    "homepage": "https://github.com/4msar/laravel-deployer",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/4msar/laravel-deployer/issues",
        "source": "https://github.com/4msar/laravel-deployer"
    },
    "authors": [
        {
            "name": "Saiful Alam",
            "email": "hello@msar.me"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/http": "^12.17",
        "laravel-zero/framework": "^12.0.2",
        "laravel-zero/phar-updater": "^1.4"
    },
    "require-dev": {
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "laravel-deployer"
    ]
}
